{
    "manifest_version": 3,
    "name": "My Extension",
    "version": "1.0",
    "description": "Description for my extension",
    "host_permissions": ["http://127.0.0.1:5000/"],
    "background": {
      "service_worker": "scripts/background.js"
    },
    "action" : {
      "default_popup": "index.html" ,
      "default_title" : ""
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["scripts/script.js"]
      }
    ]
  }
  